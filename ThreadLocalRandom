package Exercises;

import java.util.concurrent.ThreadLocalRandom;

public class RandomNumber {
    public static void main(String[] args) {
        int[] array = new int[10];
        for (int i = 0; i < 10; i++) {
            array[i] = ThreadLocalRandom.current().nextInt(0,100);
            System.out.print(array[i] + " ");
        }
    }
}
